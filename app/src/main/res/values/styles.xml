<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">


    <style name="Icon40">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:scaleType">center</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="Icon40WithDefaultBackground" parent="Icon40">
        <item name="android:background">@color/diaro_default</item>
    </style>

    <style name="Icon48">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:scaleType">center</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="DialogButtonsLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:background">#666666</item>
    </style>

    <style name="TopLineText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#F7F7F7</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:visibility">gone</item>
        <item name="android:drawablePadding">2dp</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="CalendarNavigationItem">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="DayHeader">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">none</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:paddingTop">1dp</item>
        <item name="android:paddingBottom">1dp</item>
        <item name="android:background">@color/entry_edit_mode</item>
    </style>

    <style name="DayContainer">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_marginBottom">1dp</item>
        <item name="android:layout_marginLeft">1dp</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="Day">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="DayMarkers">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_marginBottom">2dp</item>
    </style>

    <style name="SidemenuHeaderTitle">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">8dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="SidemenuSeparator">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:background">@color/sidemenu_icon_separator</item>
    </style>

    <style name="LockscreenNumbersRow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="LockscreenNumberBase">
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="LockscreenNumber" parent="LockscreenNumberBase">
        <item name="android:layout_width">70dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="EntryListIcons" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/grey_6d</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginRight">3dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="FeatureContainer">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">10dp</item>
    </style>

    <style name="Feature">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="FeatureIcon">
        <item name="android:layout_width">64dp</item>
        <item name="android:layout_height">64dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingStart">5dp</item>
        <item name="android:paddingEnd">5dp</item>
    </style>

    <style name="FeatureHeader" parent="@android:style/TextAppearance">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="FeatureDescription" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
    </style>


    <style name="FeatureContainerNew">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">start|center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/shape_rounded_rect</item>
    </style>

    <style name="FeatureHeaderNew" parent="@android:style/TextAppearance">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:fontFamily">@font/montserrat_bold</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textColor">#FE7357</item>
    </style>

    <style name="FeatureDescriptionNew" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">11sp</item>
        <item name="android:fontFamily">@font/montserrat_regular</item>
        <item name="android:letterSpacing">0.02</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:textColor">@color/txtColor1</item>
    </style>

    <style name="UserStatisticsItemTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/md_grey_300</item>
        <item name="android:textSize">12sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="EntryParamTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginStart">2dp</item>
        <item name="android:layout_marginEnd">@dimen/default_side_padding</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:padding">0dp</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="EntryIconParamTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginStart">@dimen/default_side_padding</item>
        <item name="android:layout_marginEnd">2dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:padding">5dp</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="EntryPhotoThumb">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="EntrySmallPhotoThumb">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="FormButton">
        <item name="android:layout_width">260dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:paddingRight">12dp</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="FormEditText">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:padding">4dp</item>
        <item name="android:imeOptions">flagNoExtractUi</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="FormLink">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/diaro_default</item>
        <item name="android:padding">2dp</item>
        <item name="android:clickable">true</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="FromToDateField">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textAppearance">@android:style/TextAppearance.Medium</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="ClickArea">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/bg_ripple</item>
    </style>

    <style name="WidgetColumn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="WidgetSeparator">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:background">#30ffffff</item>
    </style>


    <style name="MyAlertDialogButtonStyle" parent="@style/Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
    </style>

    <style name="MyDialogFadeAnimation">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>

    <style name="StatsCard">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginEnd">10dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:layout_marginBottom">6dp</item>
        <item name="cardCornerRadius">3dp</item>
        <item name="cardElevation">3dp</item>
    </style>

    <style name="StatsChartInfoTV">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textAppearance">@android:style/TextAppearance</item>
        <item name="android:textSize">16sp</item>
    </style>


    <!-- Text -->
